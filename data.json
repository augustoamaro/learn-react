{
  "openapi": "3.0.1",
  "info": {
    "title": "Swagger",
    "description": "Datafleet API documentation",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080 - Local"
    },
    {
      "url": "https://datafleet.io:44380/v1 - Production"
    }
